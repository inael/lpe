</head>
<body onunload="deleteAllCookies()">
    <div class="sitebody">
        <div class="conteudotop"></div>
        <div id="ouralert"></div>
        <div id='geral' class='appbody'>
            <div style="height: 20px; max-height: 20px;">
                <div id="siteContent" style="min-height: 20px; width: 100%; display:block;min-width: 160px;">
                    <table style="border-collapse: collapse; width: 100%;" align="center">
                        <tr>
                           <td valign="top">
                             <div id="topLogon">
                                   <div id="logo">
                                       <img src="/Content/imgs/logo.png" width="262px" height="105px" />
                                   </div>
                                <!--<td id="logOntd" valign="top">-->
                                    <!-- Ocultador 
                                    <div class="splitter" style="float: right;" onclick="$('#optionsmenu').toggle(); $(window).resize();">
                                        <div class="vsplitter"></div>
                                    </div>-->
                                    <div id="logOn">
                                        <img src="/Content/imgs/ajax-loader.gif" />
                                    </div>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td width="100%" valign="top">
                                <!--<div style="width: 100%; min-height: 8px; background-color: whitesmoke;"></div>-->
