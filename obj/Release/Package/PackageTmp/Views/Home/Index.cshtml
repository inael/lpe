                                <div class="conteudo">
                                    @*<div id="Menus">
                                        <img src="/Content/imgs/ajax-loader.gif" />
                                    </div>*@
                                    <div id="conteudo">
                                        @Html.Partial("Overview")
                                    </div>
                                </div>
                                <!-- rodape -->
                                <div class="rodapeTotal">
                                    <div align="right">
                                        <a href="http://www.futuriti.net.br">
                                            <img src="/Content/imgs/logoTransparente.png" height="25px" title="Desenvolvido por Futuriti" /></a></div>
                                </div>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!--jQuery-->
    <script type="text/javascript" src="/Content/Scripts/jquery-1.6.1.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery-ui-1.8.13.custom.min.js"></script>
    <!--jQuery adds-->
    <script type="text/javascript" src="/Content/Scripts/jquery.fcbkcomplete.js"> </script>
    <script type="text/javascript" src="/Content/Scripts/jquery.ui.datepicker-pt-BR.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.history.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.tooltip.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.validate.min.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.validate.messages_ptbr.js"></script>
    <!--jQuery Conversor from HTML Table to CSV format-->
    <script type="text/javascript" src="/Content/Scripts/jquery.table2csv.0.1.1.min.js"></script>
    <!--Multi-Threading-->
    <script type="text/javascript" src="/Content/Scripts/OAT/Thread.js"></script>
    <!--OAT-->
    <script type="text/javascript">        var featureList = ["pivot"];</script>
    <script type="text/javascript" src="/Content/Scripts/OAT/loader.js"></script>
    <!--jqGrid-->
    <script type="text/javascript" src="/Content/Scripts/grid.locale-pt-br.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.jqGrid.min.js"></script>
    <!--Google -->
    <script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=false"></script>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
        google.load("visualization", "1", { packages: ["corechart"] });
    </script>
    <!--Implanta-->
    <script type="text/javascript" src="/Content/Scripts/ajaxUpdate.js"></script>
    <script type="text/javascript" src="/Content/Scripts/PivotTable.js"></script>
    <script type="text/javascript" src="/Content/Scripts/ValidationAndMask.js"></script>
    <script type="text/javascript" src="/Content/Scripts/jquery.maskedinput-1.3.min.js"></script>
    <script type="text/javascript" src="/Content/Scripts/index.js"></script>
    <script type='text/javascript'>
        var indexPage = 0;
        var sessionUser = '@Session["userKey"]';
        
        if (sessionUser == ""){
            //alert(sessionUser + "LogOn");
            initializeOptionMenu('#logOn');
        }
        else {
            //alert(sessionUser + "Content");
            initializeOptionMenu('#conteudo');
            UpdateContainer('Home', 'LogOn', '#logOn', null)
        };

        $(document).ready(function () {
            $.history.init(HistoryContentUpdate, { unescape: true });
            $.ajaxSetup({
                error: trataAjaxError
            });

            //initializeOptionMenu('#logOn');

        });
    
    </script>
    
</body>
</html>
