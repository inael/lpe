<div id="_div" style="display: none;">
    <div id="boxEsquerda">
        <form id="addForm" name="add" action="" method="post">
        <div class="pageheader">
            <input id="updateButton" type="button" value="Gravar" class="btsave" />
            <input id="backButton" type="button" value="Voltar" onclick="callBackButton('EnderecoManager','List')" class="btback" />
        </div>
        <fieldset>
            <legend>Formulário de Endereços</legend>
            
            <label id= "LogradouroLabel" for="LogradouroInput">Logradouro</label>
            <input id="LogradouroInput" name="Logradouro" type="text" size="60" placeholder="Digite o nome do Logradouro" required/>
            
            <label for="BairroInput">Bairro</label>
            <input id="BairroInput" name="Bairro" type="text" size="20"/>
            
            <label for="CepInput">CEP</label>
            <input id="CepInput" name="Cep" type="text" size="20"/>

            <label for="selectMunicipio[]">Municipio</label>
            <select id="selectMunicipio" size="60" name="selectMunicipio[]" title="Ex.: São Paulo" placeholder="Digite o nome do municipio" required></select>

        </fieldset>
        </form>
    </div>
</div>


<script type="text/javascript">
    var currentID;

    $(document).ready(function () {

        var validator = $("#addForm").validate({
            rules: {
                LogradouroInput: { required: true },
                BairroInput: { required: true },
                selectMunicipio: { required: true }
            },
            errorElement: "div",
            wrapper: "div",  // a wrapper around the error message 
            errorPlacement: function (error, element) { errorCSS(error, element); },
            messages: {
                LogradouroInput: "O Nome do LOGRADOURO não ficar em branco.",
                BairroInput: "O Nome do BAIRRO não pode ficar em branco.",
                selectMunicipio: "O nome do MUNICIPIO não pode ficar em branco"
            }
        });

        $("#selectMunicipio").fcbkcomplete({
            json_url: 'AutoComplete.aspx/AutoCompleteModel.CityComplete,AutoCompleteModel,MUNICIPIOS,NOME,ID_MUNICIPIO',
            cache: true,
            filter_case: false,
            filter_selected: true,
            height: 10,
            maxitems: 1,
            complete_text: "Enter Cities..."
        });

    });

</script>