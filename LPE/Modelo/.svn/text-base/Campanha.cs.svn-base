using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.ComponentModel.DataAnnotations;

namespace Modelo
{
    public class Campanha
    {
        public virtual int Id { get; set; }                     //[ID]                  NUMERIC (18)  IDENTITY (1, 1) NOT NULL,
        public virtual string NomeCampanha { get; set; }        //[NOME]                NVARCHAR (50) NOT NULL,
        public virtual Empresa EmpresaCampanha { get; set; }    //[ID_EMPRESA]          NUMERIC (18)  NOT NULL,
        public virtual string UrlImagemCampanha { get; set; }   //[IMAGEM]              NVARCHAR (50) NOT NULL,
        public virtual DateTime DataInicio { get; set; }        //[DATA_INICIO]         DATETIME        NULL,
        public virtual DateTime DataFim { get; set; }           //[DATA_FIM]            DATETIME        NULL,
        public virtual string UsuarioInclusao { get; set; }     //[USUARIO_INCLUSAO]    NVARCHAR (30)   NULL,
        public virtual DateTime DataInclusao { get; set; }      //[DATA_INCLUSAO]       DATETIME        NULL,
        public virtual string UsuarioAteracao { get; set; }     //[USUARIO_ALTERACAO]   NVARCHAR (30)   NULL,
        public virtual DateTime DataAteracao { get; set; }      //[DATA_ALTERACAO]      DATETIME        NULL,
        public virtual bool Excluido { get; set; }              //[EXCLUIDO]            NUMERIC (18)    NULL,
        public virtual IList<Carrinho> CarrinhoCampanha { get; set; }
        public virtual IList<Pedidos> PedidosCampanha { get; set; }
    }
}
