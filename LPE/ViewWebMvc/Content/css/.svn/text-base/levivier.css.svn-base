#direita{float:right;width:210px;height:560px;margin:5px 30px 0 0;}
#rodape{float:left;background:url(../img/bg_login.jpg);width:1024px;height:150px;}
#links_rodape_int{float:left;height:30px;width:550px;font-size:10px; padding-top:10px; margin:70px 0 0 80px }
#links_rodape_int a{color:#6a6863;}
#row_2{float:right; height:80px;width: 1px;background-color:#bbae79; margin:50px 70px 0 0 ;}
#row_3{float:left; height:200px;width:1px;background-color:#bbae79;margin:32px 0 0 10px ;}
#pagseguro{float:right;width:230px; height:90px; background-image:url(../img/pagseguro.png); background-repeat:no-repeat;margin:40px 50px 0 0 }
#selecionado{float:left;width:150px; height:255px;font-size:9px;}
#selecionado ul{float:left;margin-left:-35px;}
#lista_selecionado{float:left;width:785px;height:730px;margin:-23px 0 0 -40px;}
#lista_selecionado li{float:left;width:220px;height:330px;color:#6a6863;font-size:11px; margin:25px 0 0 25px}
#map{float:left;font-size:12px;color:#6a6863;margin:230px 0 0 -350px }
#ver_mais{float:right;font-size:10px;color:#6a6863; margin:230px -270px 0 0}
#ver_mais a{color:#bbae79}
#tabelas_admin{margin:30px 0 0 20px; font-size:16px;}
#tabelas_admin .table_mais{float:left; margin:15px 0 0 15px}
#tabelas_admin input{border:none; text-align:left;}
#tabelas_admin input:hover{cursor:pointer}
#tabelas_admin .table_menos{float:left; margin:75px 0 0 -300px}


