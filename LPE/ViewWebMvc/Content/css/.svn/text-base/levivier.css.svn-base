#links_rodape{float:left; border-top:1px solid #bbae79;height:30px;width:250px;
font-size: 11px; padding-top:10px; margin:200px 0 0 10px }
#links_rodape a{color:#6a6863;}
#promo{width:280px; height:40px; float:left; margin:10px 0 0 410px;letter-spacing:2px;}
