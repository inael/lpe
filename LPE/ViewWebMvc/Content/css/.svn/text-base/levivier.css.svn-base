#promo_preco{background-color:#bbae79; width:75px; height:75px;float:left;color:#fff;}
.um, .dois, .tres{float:left;font-family:Arial,Helvetica,sans-serif;}
.um{margin:17px 0 0 7px;font-size:35px; font-weight:lighter}
.dois{margin:27px 0 0 0;font-size:35px; font-weight:lighter}
.tres{margin:37px 0 0 6px;font-size:20px; font-weight:lighter}
#esquerda_topo{width:680px; height:auto; float:left; margin-top:-30px;}
#banner_destaque{background:url(../img/banner_femi.jpg) no-repeat;width:515px; height:185px; margin:18px 0 0 0 }
#data_dest{background-color:#bbae79; color:#fff; font-size:12px; width:180px; height:18px;float:left;}
#marca_topo{float:right; width:105px; height:135px; font-size:9px; letter-spacing:1px;margin:-185px 30px 0 0}
