/****
--- cores ---
#bbae79 --> bege botão de pesquisa, titulo destaque semana, linhas, #promo preço;
#696039 --> links topo esquerdo;
#6a6863 --> 'esqueceu a senha', prox. ofertas, links rodape -> bege mais escuro
****/
a{text-decoration: none;}
li{list-style:none;}
img {
  border: none;
}
body {
  margin: 0;padding: 0;border: 0; /*background:url(../imagens/bg.jpg) center;*/
  font-family: "Segoe UI Web Semibold", "Segoe UI Web Regular", "Segoe UI", "Helvetica Neue", Arial;
}
#conteiner_geral {width:1035px;min-height:775px;margin: 0 auto; }
#topo_esquerdo{margin-left:20px; float:left; width:355px; height:140px; font-family:Arial,Helvetica,sans-serif; font-size: 13px;}
#topo_esquerdo a{color:#696039}
#logo, #linhas{float:left;}
#linhas{margin:10px 0 0 20px; border-bottom:2px solid #bbae79;}
.campo{width:140px; float:left; margin:10px 0 0 20px;color:#6a6863; border:1px solid #bbae79}
.botao{width:30px; float:left; margin:10px 0 0 5px; background-color:#f1e8d1; color:#6a6863; text-align:center; border:none;}
.botao:hover{cursor:pointer}
#topo_direito{float:right; margin:12px; font-family:Arial,Helvetica,sans-serif;color:#6a6863; }
#redes_sociais{float:right; width:75px; height:auto; margin-top: -5px;}
#login{ background:url(../img/bg_login.jpg); float:left; width:245px; height:100px;}
/*campo topo direito*/
#topo_direito form{margin:10px;}
.botao2{width:30px; float:right; margin:10px 10px 0 0; background-color:#f1e8d1;text-align:center;}
.campo2{width:180px; border:none;-border-radius:10px;-webkit-border-radius:10px; -moz-border-radius:10px;font-size:10px;height:20px; }
#login label{margin:10px 0 0 15px; float:left; font-size:12px;font-weight: bold}
.err_login{margin:0 0 0 12px; font-size:11px; float:left}
#sacola{font-size:11px;width:100px;height:30px; float:left; margin:10px 0 0 160px;}
#sacola img{padding-top:-20px;float:left;}
#sacola .text{float:left;padding:10px 0 0 0;}
#centro, #centro_interno,#tabelas_admin{width:1024px; height:420px; float:left;margin-top:100px; font-family:Arial,Helvetica,sans-serif;}
#centro_interno{margin:10px 0 0 20px;}
#foto
{
	border:0px solid black; float:left; margin:-180px 0 0 20px; z-index:9999;/* width:250px; height:250px; 
    border-radius: 210px; /*(padrão) 
	-moz-border-radius: 210px; /* (navegadores Flock e Firefox) 
	-webkit-border-radius: 210px; /*(navegadores Chrome e Safari)*/
}
#bg_centro{background-color:#e7e5df;width:1024px;height:105px}
.destaque_titulo{letter-spacing:10px; float:left; margin:-25px 0 0 370px;color:#bbae79;}
#bg_mais_detalhes{letter-spacing:3px; background-color:#f3f0e9;color:#cac097; font-size:11px; 
width:180px;height:15px;float:left; margin-left:-5px; z-index:-1; padding:5px 0 0 20px;}
#lista_destaques{float:left; width:640px;height:150px;margin-left:-20px;}
#lista_destaques li{display:inline;width:150px;height:155px;font-size:10px;color:#000;float:left}
.marca_lista{float:right; margin:-10px 5px 0 0 }
#row{background-color:#bbae79; float:left; width:300px;height:1px; margin-left:250px;}
.prox_of{float:left;color:#6a6863;font-size:11px;margin:10px 0 0 -200px;letter-spacing:3px;}
#marcas_prox{float:right;width:660px;height:30px;margin:5px 50px 0 0;}
