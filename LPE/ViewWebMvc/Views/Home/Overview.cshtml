@{
    var Message = TempData["Message"];
    var MessageStatus = TempData["MessageStatus"];    
}
<input type="text" value="@Message" name="Message" id="Message" style="display:none;" />
<input type="text" value="@MessageStatus" name="MessageStatus" id="MessageStatus" style="display:none;" />
<script type="text/javascript" runat="server">  
    var msg = document.getElementById('Message'); 
	if(msg.value != "") { 
		alert(msg.value); 
    } 

    var msgStatus = document.getElementById('MessageStatus');     
	if(msgStatus.value == "401") { 
        logoutPage();		
    }     
</script>
