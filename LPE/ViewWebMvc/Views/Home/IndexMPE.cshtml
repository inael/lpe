    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/admin.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/adminIndex.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/adminDetalhes.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/adminCreate.css" />
     <!--jQuery-->
