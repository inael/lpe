    <script type="text/javascript">        var featureList = ["pivot"];</script>
    <script type="text/javascript" src="/LPE/Content/Scripts/OAT/loader.js"></script>
    <!--jqGrid-->
    <script type="text/javascript" src="/LPE/Content/Scripts/grid.locale-pt-br.js"></script>
    <script type="text/javascript" src="/LPE/Content/Scripts/jquery.jqGrid.min.js"></script>
    <!--Google -->
    <script type="text/javascript" src="https://maps.google.com/maps/api/js?sensor=false"></script>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
        google.load("visualization", "1", { packages: ["corechart"] });
    </script>
    <!--Futuriti-->
