    <script type="text/javascript" src="/LPE/Content/Scripts/CrudMenu.js"></script>
    <script type="text/javascript" src="/LPE/Content/Scripts/GlobalFunctions.js"></script>
    <script type='text/javascript' src="/LPE/Content/Scripts/IndexMPEOptionMenu.js"></script>
</head>
<body>
    <div class="sitebody">
        <div class="conteudotop"></div>
        <div id="ouralert"></div>
        <div id='geral' class='appbody'>
            <div style="height: 20px; max-height: 20px;">
                <div id="siteContent" style="min-height: 20px; width: 100%; display:block; min-width: 160px; background-color:#fff;">
                    <table style="border-collapse: collapse; width: 100%;" align="center">
                        <tr>
                           <td>
                             <div id="topLogon">
                                   <div id="logo"></div>
                                   <div id="userName" style="padding: 10px; display:none">
                                       <input id="btnLogout" class="btlogout" type="button" value="Sair"  />
                                   </div>
                                   <br />
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td width="100%" valign="top">
                                <!--<div style="width: 100%; min-height: 8px; background-color: whitesmoke;"></div>-->
                                <!-- Centro -->
                                <div class="conteudo">
                                    <div id="conteudo">
                                        @Html.Partial("Overview")
                                    </div>
                                </div>
                                <div id="logOn" style="border:0px solid green; left:67%; top:90%; float:left; position: absolute;">
                                    <img src="/LPE/Content/imgs/ajax-loader.gif" />
                                </div>
                                <!-- rodape -->
                                <div class="rodapeTotal">
                                    <div align="right">
                                        <a href="http://www.futuriti.net.br">
                                            <img src="/LPE/Content/imgs/logoTransparente.png" height="25px" title="Desenvolvido por Futuriti" /></a></div>
                                </div>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
   
