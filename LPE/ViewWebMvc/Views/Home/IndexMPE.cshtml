    var a;   
}
    <title>LPE</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <!-- Estilos -->
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/facebookstyle.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/smoothness/jquery-ui-1.8.13.custom.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/estilo.css" />
    <link rel="stylesheet" type="text/css" href="/LPE/Content/css/ui.jqgrid.css" />
